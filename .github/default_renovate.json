{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["dev"],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "branchPrefix": "deps/",
  "extends": [
    "config:base"
  ],
  "reviewers": [
    "RainbowDashLabs"
  ],
  "prConcurrentLimit": 3,
  "prHourlyLimit": 10,
  "packageRules": [
    {
      "matchDepNames": ["gradle"],
      "automerge": true,
      "squash": true
    },
    {
      "matchPackagePatterns": ["io.papermc.paper:paper-api", "org.spigotmc:spigot-api", "com.github.seeseemelk:MockBukkit-.+"],
      "enabled": false
    }
  ]
}
